{
  "schemaVersion": 1,
  "id": "pas",
  "version": "${version}",

  "name": "Planets And Stars",
  "description": "A very modular space mod",
  "authors": [
    "Rochedo"
  ],
  "contact": {
  },

  "license": "Apache-2.0",
  "icon": "assets/pas/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.github.rochedo.planetsandstars.PlanetsAndStars"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.github.rochedo.planetsandstars.PlanetsAndStarsClient"
      }
    ]
  },
  "mixins": [
    "pas.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.8.7",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.17.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
